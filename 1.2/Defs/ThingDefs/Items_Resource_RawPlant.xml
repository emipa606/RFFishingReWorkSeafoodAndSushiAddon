<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="PlantFoodRawBase">
    <defName>RF_RawSoybean</defName>
    <label>soybeans</label>
    <description>Raw soybean. Ingredients for sushi soy sauce. It is also as an ingredient for cooking.</description>
    <soundInteract>Grain_Drop</soundInteract>
    <soundDrop>Grain_Drop</soundDrop>
    <graphicData>
      <texPath>Things/Items/RawPlant/RF_RawSoybean</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.1</MarketValue>
    </statBases>
    <ingestible>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>60</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>
  <ThingDef ParentName="RoughPlantBase">
    <defName>RF_RawWasabi</defName>
    <label>wasabi</label>
    <description>It is a raw material for sushi wasabi paste. It's not recommended to eat it as it is.</description>
    <soundInteract>Grain_Drop</soundInteract>
    <soundDrop>Grain_Drop</soundDrop>
    <graphicData>
      <texPath>Things/Items/RawPlant/RF_RawWasabi</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.8</MarketValue>
    </statBases>
    <ingestible>
      <preferability>DesperateOnly</preferability>
      <canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <thingCategories>
      <li>PlantMatter</li>
    </thingCategories>
  </ThingDef>
</Defs>
