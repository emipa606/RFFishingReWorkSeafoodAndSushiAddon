<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- nigiri sushi  -->
  <RecipeDef Name="RF_CookMealSushiBase" Abstract="True">
    <researchPrerequisite>RFF_MakeSeafood</researchPrerequisite>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <requiredGiverWorkType>Cooking</requiredGiverWorkType>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawRice</li>
          </thingDefs>
        </filter>
        <count>0.25</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>RF_SoySauce</li>
          </thingDefs>
        </filter>
        <count>0.05</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>RF_WasabiPaste</li>
          </thingDefs>
        </filter>
        <count>0.05</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawRice</li>
        <li>RF_SoySauce</li>
        <li>RF_WasabiPaste</li>
      </thingDefs>
    </fixedIngredientFilter>
    <workSkill>Cooking</workSkill>
    <recipeUsers>
      <li>RF_SushiCounter</li>
    </recipeUsers>
    <skillRequirements>
      <Cooking>10</Cooking>
    </skillRequirements>
  </RecipeDef>
  <!-- nigiri squid -->
