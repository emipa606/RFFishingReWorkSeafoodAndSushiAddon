    </ingredients>
    <products>
      <MealSushiSquid>1</MealSushiSquid>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawSquidMeat</li>
      </thingDefs>
    </fixedIngredientFilter>
  </RecipeDef>
